{
  "name": "vscode-drib-language",
  "displayName": "Drib Language",
  "description": "The Drib Programming Language",
  "version": "0.0.2",
  "publisher": "drib-lang",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/drib-lang/vscode-drib-lang"
  },
  "contributes": {
    "languages": [
      {
        "id": "drib",
        "aliases": [
          "Drib",
          "drib"
        ],
        "extensions": [
          ".drib"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "drib",
        "scopeName": "source.drib",
        "path": "./syntaxes/drib.json"
      }
    ]
  }
}