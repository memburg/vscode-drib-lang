{
  "name": "drib-language-basics",
  "displayName": "Drib Language Basics",
  "description": "Basic syntax highlighting for the Drib language.",
  "version": "0.0.1",
  "publisher": "your-publisher-id",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "drib",
        "aliases": ["Drib", "drib"],
        "extensions": [".drib"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "drib",
        "scopeName": "source.drib",
        "path": "./syntaxes/drib.json"
      }
    ]
  }
}
